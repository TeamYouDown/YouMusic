<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="repeat_hide"
            android:width="24dp"
            android:height="24dp"
            android:tint="?invertedColor"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="line_top"
                android:pathData="M 6 11 L 6 6 L 18 6 L 18 18 L 6 18 Z"
                android:strokeWidth="2"
                android:strokeColor="?invertedColor"
                android:strokeLineCap="square"
                android:trimPathEnd="0.35416" />
            <group
                android:name="arrow_top"
                android:pivotX="18"
                android:pivotY="6">
                <path
                    android:name="arrow_top_path"
                    android:fillColor="?invertedColor"
                    android:pathData="M 17 2 L 17 10 L 21 6 L 17 2 Z"
                    android:strokeWidth="1" />
            </group>
            <group
                android:name="arrow_bottom"
                android:pivotX="6"
                android:pivotY="18">
                <path
                    android:name="arrow_bottom_path"
                    android:fillColor="?invertedColor"
                    android:pathData="M 7 17 L 7 14 L 3 18 L 7 22 L 7 17 Z"
                    android:strokeWidth="1" />
            </group>
            <path
                android:name="line_bottom"
                android:pathData="M 18 13 L 18 18 L 6 18 L 6 6 L 18 6 Z"
                android:strokeWidth="2"
                android:strokeColor="?invertedColor"
                android:strokeLineCap="square"
                android:trimPathEnd="0.35416" />
            <path
                android:name="label_one"
                android:fillColor="?invertedColor"
                android:pathData="M 13 15 L 13 9 L 12 9 L 10 10 L 10 11 L 11.5 11 L 11.5 15"
                android:strokeWidth="1"
                android:strokeLineCap="square" />
        </vector>
    </aapt:attr>
    <target android:name="arrow_top">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="60"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="90"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="60"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="rotation"
                    android:startOffset="110"
                    android:valueFrom="90"
                    android:valueTo="180"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/fast_out_linear_in"
                    android:propertyName="translateY"
                    android:valueFrom="0"
                    android:valueTo="12"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="translateX"
                    android:startOffset="130"
                    android:valueFrom="0"
                    android:valueTo="-12"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="line_top">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/fast_out_linear_in"
                    android:propertyName="trimPathEnd"
                    android:valueFrom="0.35416"
                    android:valueTo="0.6041"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="trimPathEnd"
                    android:startOffset="130"
                    android:valueFrom="0.6041"
                    android:valueTo="0.85416"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/fast_out_linear_in"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0"
                    android:valueTo="0.25"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="trimPathStart"
                    android:startOffset="130"
                    android:valueFrom="0.25"
                    android:valueTo="0.5"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="arrow_bottom">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="60"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="90"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="60"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="rotation"
                    android:startOffset="110"
                    android:valueFrom="90"
                    android:valueTo="180"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/fast_out_linear_in"
                    android:propertyName="translateY"
                    android:valueFrom="0"
                    android:valueTo="-12"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="translateX"
                    android:startOffset="130"
                    android:valueFrom="0"
                    android:valueTo="12"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="line_bottom">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/fast_out_linear_in"
                    android:propertyName="trimPathEnd"
                    android:valueFrom="0.35416"
                    android:valueTo="0.6041"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="trimPathEnd"
                    android:startOffset="130"
                    android:valueFrom="0.6041"
                    android:valueTo="0.85416"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/fast_out_linear_in"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0"
                    android:valueTo="0.25"
                    android:valueType="floatType" />
                <objectAnimator
                    android:duration="130"
                    android:interpolator="@android:interpolator/linear_out_slow_in"
                    android:propertyName="trimPathStart"
                    android:startOffset="130"
                    android:valueFrom="0.25"
                    android:valueTo="0.5"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="label_one">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="230"
                android:interpolator="@android:anim/linear_interpolator"
                android:propertyName="fillAlpha"
                android:valueFrom="1"
                android:valueTo="0"
                android:valueType="floatType" />
        </aapt:attr>
    </target>
    <target android:name="repeat_hide">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="230"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="alpha"
                android:valueFrom="1"
                android:valueTo="0.5"
                android:valueType="floatType" />
        </aapt:attr>
    </target>
</animated-vector>
